#Acitivity 8
SELECT FirstName, LastName, City
FROM Client
WHERE City IS 'Metairie','Kenner','Gretna'

#Acitivity 9
SELECT FirstName, LastName,  BirthDate 
FROM Client
WHERE BirthDate > '1980-01-01'

#Acitivity 10
SELECT FirstName, LastName,  BirthDate 
FROM Client
WHERE BirthDate BETWEEN '1980-01-01' AND ' 2022-01-01'

#Activity 11
SELECT EmailAddress
FROM Login
WHERE EmailAddress LIKE '%.gov'

#Acitivity 12
SELECT EmailAddress
FROM Login
WHERE EmailAddress NOT LIKE '%.com'

#Activity 13
SELECT FirstName, LastName
FROM Client
WHERE BirthDate IS NULL

#Activity 14
SELECT Name
FROM ExerciseCategory 
WHERE ParentCategoryID IS NOT NULL

#Activity 15
SELECT Name, Notes
FROM Workout
WHERE LevelId IS 3 
AND Notes LIKE '%you%';

#Acitivity 16
SELECT EmailAddress 
FROM Login 
WHERE clientid=(SELECT clientid FROM client WHERE firstname='Estrella' and lastname='Bazely');

#Activity 17
SELECT InvoiceId, Description, Price, Quantity, ServiceDate, Price*Quantity AS line_item_total
FROM InvoiceLineItem
WHERE line_item_total BETWEEN 15 AND 25

